{
  "name": "starter-kit",
  "version": "0.0.1",
  "description": "Starter kit for full-stack JavaScript projects",
  "private": true,
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config client/webpack/prod.config.js",
    "build:server": "babel server --out-dir dist",
    "cleanup": "rimraf ./dist/*",
    "dev": "concurrently -k -n \"client,server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack serve --config client/webpack/dev.config.js",
    "dev:server": "cross-env PORT=3100 nodemon --exitcrash --inspect --delay 500ms --watch server --exec babel-node server/server.js",
    "lint": "eslint .",
    "preserve": "npm run build",
    "serve": "npm start",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/starter-kit.git"
  },
  "author": "Jonathan Sharpe <mail@jonrshar.pe>",
  "contributors": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/starter-kit/issues"
  },
  "homepage": "https://github.com/textbook/starter-kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/node": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@codeyourfuture/eslint-config-standard": "^2.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "html-webpack-tags-plugin": "^3.0.1",
    "nodemon": "^2.0.7",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "wait-on": "^5.3.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.5",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "axios": "^0.21.1",
    "cookie-session": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "knex": "^0.95.6",
    "knex-migrate": "^1.7.4",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "react-chartjs-2": "^3.0.3"
  },
  "engines": {
    "node": "^10 || ^12 || ^14 || ^16"
  }
}
